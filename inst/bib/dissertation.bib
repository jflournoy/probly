  author = {Nelson, Eric E. and Jarcho, Johanna M. and Guyer, Amanda E.},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/BZM42BQ2/Nelson_et_al-2016-Social_re-orientation_and_brain_development.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/37TSVKZ7/S1878929315001279.html;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/JBDRZZJG/S1878929315001279.html}
