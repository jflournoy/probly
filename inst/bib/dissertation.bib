  journaltitle = {Journal of Child Psychology and Psychiatry},
  shortjournal = {J Child Psychol Psychiatr},
  urldate = {2016-10-01},
  date = {2016-03-01},
  pages = {353-368},
  keywords = {Adolescence,Brain connectivity,Individual Differences,ventral striatum,Risk-taking behavior,developmental changes,externalizing disorders,_tablet},
  author = {Crone, Eveline A. and van Duijvenvoorde, Anna C. K. and Peper, Jiska S.},
  options = {useprefix=true},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/S76FDXAQ/Crone_et_al-2016-Annual_Research_Review.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/ZQ7QWXEH/abstract.html}
