  keywords = {college students,College freshmen,Initiation,Marijuana,Drugs,_tablet},
  author = {Suerken, Cynthia K. and Reboussin, Beth A. and Sutfin, Erin L. and Wagoner, Kimberly G. and Spangler, John and Wolfson, Mark},
  file = {/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/2UN8WMRB/Suerken_et_al-2014-Prevalence_of_marijuana_use_at_college_entry_and_risk_factors_for_initiation.pdf;/home/jflournoy/.zotero/zotero/n9n1b2c7.default/zotero/storage/MGGPC8GH/S0306460313003201.html}
